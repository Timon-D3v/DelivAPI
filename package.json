{
  "name": "delivapi",
  "version": "1.1.0",
  "description": "A personal CDN/WebAPI for uploading and pulling images and other files from.",
  "homepage": "https://github.com/Timon-D3v/DelivAPI.git#readme",
  "author": "Timon Fiedler",
  "license": "MIT",
  "main": "src/app.ts",
  "type": "module",
  "types": "src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Timon-D3v/DelivAPI.git"
  },
  "bugs": {
    "url": "https://github.com/Timon-D3v/DelivAPI.git/issues"
  },
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "tsc",
    "watch": "tsc --watch --noEmit",
    "start": "tsc && node dist/app.js",
    "start:last": "node dist/app.js",
    "format": "prettier --write \"{src,public}/**/*.{ts,html,css,js}\"",
    "docker:build": "docker build . -t delivapi:latest",
    "docker:run": "docker run --rm --name delivapi -p 8080:8080 --env-file ./docker.env delivapi",
    "docker:stop": "docker stop delivapi"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "delivapi-client": "^1.0.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "multer": "^2.0.1",
    "mysql2": "^3.14.1",
    "node-mailjet": "^6.0.8",
    "timonjs": "^2.1.0"
  },
  "devDependencies": {
    "@types/multer": "^2.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.6.0",
    "prettier": "^3.5.3",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "API",
    "CDN",
    "file",
    "upload",
    "pull",
    "image",
    "webapi",
    "personal"
  ]
}
